{
  "version": 2,
  "builds": [
    {
      "src": "project/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "app/staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/vercel/path0/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "project/wsgi.py"
    }
  ],
  "outputDirectory": "app/staticfiles"
}
